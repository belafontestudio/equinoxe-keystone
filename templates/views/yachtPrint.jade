extends ../layouts/print


block content
  .newpage
    #header.big(style="display:none;")
      img#logo(src="data:image/png;base64,")
    #cover(style="margin-top:-80px")
      if data.yacht.cover.filename
        img(src= data.url+"/uploads/images/yachts/covers/"+data.yacht.cover.filename, alt=data.yacht.cover.filename)
      else 
        img(src= data.url+"/images/sprite/placeholder_cover.png", alt="placeholder")
    
    section.center
      
      div#container-other-specs
        .main-text  
          h1 
            block title
              = data.yacht.name
        .clear
        
      .top-specs
        .top-specs-little
          ul#top-specs-left
            if data.yacht["price per week from"] || data.yacht.price
              li
                if data.yacht.availability == "Charter"
                  span
                    | Charter price from:
                  = numeralFunction(data.yacht["price per week from"]).format('0,0[.]00' )+data.yacht.currency
                  
                if data.yacht.availability == "Sale"
                  if data.yacht.price
                    span
                      | Sale price:
                    = numeralFunction(data.yacht.price).format('0,0[.]00' )+data.yacht.currency
                    
            if data.yacht.lenght
              li
                span
                  | length:
                block single-yacht-length
                  = data.yacht.lenght+"m"
                  
            if data.yacht.build || data.yacht.refit 
              li
                if data.yacht.refit && data.yacht.build
                  span
                    | built:
                  = data.yacht.build+" / refit "+data.yacht.refit
                else
                    if data.yacht.refit
                      span
                        | refit:
                      = data.yacht.refit
                    else if data.yacht.build
                      span
                        | built:
                      = data.yacht.build 
            if data.yacht.guests
              li
                span
                  | guests:
                block single-yacht-guests
                  = data.yacht.guests

          ul#top-specs-right
            h3
              | Accomodation
            if data.yacht.guests
              li
                span
                  | guests:
                 =data.yacht.guests
            if data.yacht.crew
              li
                span
                  | crew:
                 =data.yacht.crew
            if data.yacht.cabins
              li
                span
                  | cabins:
                 =data.yacht.cabins
        
        

       

      div.bottom-specs
            div#other-specs
              h3
                | Specifications
              ul.other-specs-list
                if data.yacht.beam
                  li
                    span
                      | beam:
                    =data.yacht.beam
                if data.yacht.crew
                  li
                    span
                      | number of crew:
                    =data.yacht.crew
                if data.yacht.builder
                  li
                    span
                      | builder:
                    =data.yacht.builder
                if data.yacht.designer
                  li
                    span
                      | designer:
                    =data.yacht.designer
                if data.yacht.flag
                  li
                    span
                      | flag:
                    =data.yacht.flag
                if data.yacht.model
                  li
                    span
                      | model:
                    =data.yacht.model
                if data.yacht.draft
                  li
                    span
                      | draft:
                    =data.yacht.draft
              ul.other-specs-list
                if data.yacht.displacement
                  li
                    span
                      | displacement:
                    =data.yacht.displacement
                if data.yacht["crusing speed"]
                  li
                    span
                      | Cruising speed:
                    =data.yacht["crusing speed"]
                if data.yacht.consumption
                  li
                    span
                      | Fuel consumption:
                    =data.yacht.consumption
                if data.yacht.vat
                  li
                    span
                      | VAT:
                    =data.yacht.vat
                if data.yacht.location    
                  li
                    span
                      | Location:
                    =data.yacht.location
                if data.yacht["price per week low season"]
                  li
                    span
                      | Price Per Week Low Season:
                    = numeralFunction(data.yacht["price per week low season"]).format('0,0[.]00' )+data.yacht.currency

                if data.yacht["price per week high season"]
                  li
                    span
                      | Price Per Week High Season:
                    = numeralFunction(data.yacht["price per week high season"]).format('0,0[.]00' )+data.yacht.currency
              
